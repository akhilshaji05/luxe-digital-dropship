{
    "framework": "vite",
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/wc-auth/v1/authorize",
            "destination": "/api/wc-auth"
        },
        {
            "source": "/wp-json/wc/v3(.*)",
            "destination": "/api/wc-v3"
        },
        {
            "source": "/wp-json(.*)",
            "destination": "/api/wp-json"
        },
        {
            "source": "/wp-login.php",
            "destination": "/api/mock-wp"
        },
        {
            "source": "/xmlrpc.php",
            "destination": "/api/mock-wp"
        },
        {
            "source": "/wp-admin/admin-ajax.php",
            "destination": "/api/admin-ajax"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}